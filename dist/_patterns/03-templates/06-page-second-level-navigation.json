{
    "page_header_content": {
        "join()": [
            "<div class=\"region region--header\">",
            "molecules-block--system-branding-block.html",
            "molecules-block--main-navigation",
            {
                "include()": {
                    "pattern": "molecules-block--system-menu-block.html",
                    "with": {
                        "configuration": {
                            "label_display": false
                        }
                    }
                }
            },
            "</div>"
        ]
    },
    "block_page_title": {
        "include()": {
            "pattern": "molecules-block--page-title",
            "with": {
                "content": {
                    "include()": {
                        "pattern": "molecules-page-title.html",
                        "with": {
                            "title": "Basic page"
                        }
                    }
                }
            }
        }
    },
    "block_main": {
        "include()": {
            "pattern": "organisms-block--main",
            "with": {
                "content": "organisms-page-full"
            }
        }
    }
}
