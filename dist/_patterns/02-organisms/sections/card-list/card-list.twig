<div class="card-list">
  {% block cards %}
    {% for card in cards %}
      {% include '@molecules/components/card/card.twig'
        with {
          'card_image': card.card_image,
          'card_title': card.card_title,
          'card_body': card.card_body
        }
      %}
    {% endfor %}
  {% endblock %}
</div>
