<nav role="navigation" aria-labelledby="system-breadcrumb" class="breadcrumb-trail">
  <h2 id="system-breadcrumb" class="visually-hidden">{{ breadcrumb_trail_heading }}</h2>
  <ol>
  {% for breadcrumb in breadcrumb_trail %}
    <li>
      {% if breadcrumb.url %}
        <a href="{{ breadcrumb.url }}">{{ breadcrumb.text }}</a>
      {% else %}
        {{ breadcrumb.text }}
      {% endif %}
    </li>
  {% endfor %}
  </ol>
</nav>
